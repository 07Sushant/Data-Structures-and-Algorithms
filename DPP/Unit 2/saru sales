#include <iostream>
#include <unordered_map>
#include <map>
using namespace std;

void findFrequency(int* arr, int size) {
    unordered_map<int, int> frequencyMap;
    
    // Count the frequency of each element
    for (int i = 0; i < size; i++) {
        frequencyMap[arr[i]]++;
    }
    
    // Print the frequency of each element in sorted form
    map<int, int> sortedMap(frequencyMap.begin(), frequencyMap.end());
    for (auto it = sortedMap.begin(); it != sortedMap.end(); it++) {
        if (it->second == 1) {
            cout << it->first << " - " << it->second << " time" << endl;
        } else {
            cout << it->first << " - " << it->second << " times" << endl;
        }
    }
}

int main() {
    int size;
    cin >> size;
    
    int arr[size];
    for (int i = 0; i < size; i++) {
        cin >> arr[i];
    }
    
    findFrequency(arr, size);
    
    return 0;
}
