#include <iostream>
#include <vector>
using namespace std;

vector<int>rotateArray(const vector<int>& arr,int positions)
    {
        int n=arr.size();
        positions %= n;
        vector<int> rotated(n);
        for (int i=0;i<n; ++i)
        {
            int newPos=(i + positions)%n;
            rotated[newPos] = arr[i];
        }
        return rotated;
    }
    

int main()
{
    int size;
    cin>>size;
    vector<int>arr(size);
    for (int i=0;i<size; ++i)
    {
        cin>>arr[i];
    }
    int positions;
    cin>>positions;
    cout<<"Original array: ";
    for(int num:arr)
    {
        cout<<" "<<num;
    }
    cout<<endl;
    vector<int> rotatedArr = rotateArray(arr, positions);
    cout<<"Updated array: ";
    for (int num:rotatedArr)
    {
        cout<<" "<<num;
    }
    cout<<endl;
    return 0;
}
